{
  "name": "postrouter",
  "version": "3.0.12",
  "description": "A lightweight React router",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "cross-env npm run cleanup && npm run compile",
    "cleanup": "node -e \"require('fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "react router",
    "router",
    "history"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/axtk/postrouter.git"
  },
  "author": "axtk",
  "license": "MIT",
  "dependencies": {
    "histloc": "4.0.0"
  },
  "peerDependencies": {
    "@types/react": ">16.8",
    "react": ">16.8"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "typescript": "4.7.4"
  }
}
